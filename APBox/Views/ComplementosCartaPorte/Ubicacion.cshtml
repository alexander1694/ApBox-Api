@model API.Operaciones.ComplementoCartaPorte.Ubicacion


@using (Html.BeginCollectionItem("Ubicaciones"))
{
    @Html.ValidationSummary(true, "", new { @class = "label-blue-alt" })

    <div class="card shadow-sm">
        <div class="card-header" style="background-color: #6D84C2;">
            <h3 class="card-title text-white">
                Ubicacion
            </h3>
            <div class="card-toolbar">
                <a href="#" class="icon-separator" onclick="removeDetalleUbicacion(this)">
                    <!--begin::Svg Icon -->
                    <span class="svg-icon svg-icon-3x">
                        <img src="~/_CDN/assets/media/icons/duotune/general/gen034.svg" width="30" height="30" />
                    </span>
                    <!--end::Svg Icon-->
                </a>
            </div>

        </div>
        <div class="card-body row ubicacion">


            @Html.HiddenFor(model => model.Id)
            @Html.HiddenFor(model => model.Complemento_Id)
            @Html.HiddenFor(model => model.Domicilio_Id)
            <div class="clearfix"></div>

            <div class="content-box-wrapper">
                <div class="row">
                    @if (Model.TipoUbicacion == "Origen")
                    {
                        <h3 class="title-hero">
                            Ubicación Origen
                        </h3>
                    }
                    else
                    {
                        <h3 class="title-hero">
                            Ubicación Destino
                        </h3>
                    }
                    @Html.HiddenFor(model => model.TipoUbicacion)
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.IDUbicacion, new { @class = "fw-bold" })<br />
                        @*@Html.DisplayFor(model => model.IDUbicacion)*@
                        <p id="IdUbicacion">@Model.IDUbicacion</p>
                        @Html.HiddenFor(model => model.IDUbicacion)

                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.RfcRemitenteDestinatario, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.RfcRemitenteDestinatario)
                        @Html.HiddenFor(model => model.RfcRemitenteDestinatario)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.NombreRemitenteDestinatario, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.NombreRemitenteDestinatario)
                        @Html.HiddenFor(model => model.NombreRemitenteDestinatario)
                    </div>
                    @if (Model.NumRegIdTrib != "" && Model.NumRegIdTrib != null)
                    {
                        <div class="form-group col-xs-12 col-sm-3">
                            @Html.LabelFor(model => model.NumRegIdTrib, new { @class = "fw-bold" })<br />
                            @Html.DisplayFor(model => model.NumRegIdTrib)
                            @Html.HiddenFor(model => model.NumRegIdTrib)
                        </div>
                        <div class="form-group col-xs-12 col-sm-3">
                            @Html.LabelFor(model => model.ResidenciaFiscal, new { @class = "fw-bold" })<br />
                            @Html.DisplayFor(model => model.ResidenciaFiscal)
                            @Html.HiddenFor(model => model.ResidenciaFiscal)
                        </div>
                    }
                    @if (Model.NombreEstacion != "")
                    {
                        <div class="form-group col-xs-12 col-sm-3">
                            @Html.LabelFor(model => model.NombreEstacion, new { @class = "fw-bold" })<br />
                            @Html.DisplayFor(model => model.NombreEstacion)
                            @Html.HiddenFor(model => model.NombreEstacion)

                        </div>
                        <div class="form-group col-xs-12 col-sm-3">
                            @Html.LabelFor(model => model.NumEstacion, new { @class = "fw-bold" })<br />
                            @*@Html.DisplayFor(model => model.NumEstacion)*@
                            @Html.HiddenFor(model => model.NumEstacion)
                            @Html.DisplayFor(model => model.Estaciones_Id)
                            @Html.HiddenFor(model => model.Estaciones_Id)
                        </div>
                        <div class="form-group col-xs-12 col-sm-3">
                            @Html.LabelFor(model => model.TipoEstaciones, new { @class = "fw-bold" })<br />
                            @*@Html.DisplayFor(model => model.TipoEstaciones)*@
                            @Html.HiddenFor(model => model.TipoEstaciones)
                            @Html.DisplayFor(model => model.TipoEstacion_Id)
                            @Html.HiddenFor(model => model.TipoEstacion_Id)
                        </div>
                        <div class="form-group col-xs-12 col-sm-3">
                            @Html.LabelFor(model => model.NavegacionTrafico, new { @class = "fw-bold" })<br />
                            @Html.DisplayFor(model => model.NavegacionTrafico)
                            @Html.HiddenFor(model => model.NavegacionTrafico)
                        </div>
                    }
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.FechaHoraSalidaLlegada, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.FechaHoraSalidaLlegada)
                        @Html.HiddenFor(model => model.FechaHoraSalidaLlegada)
                    </div>
                    @if (Model.TipoUbicacion == "Destino")
                    {
                        <div class="form-group col-xs-12 col-sm-3">
                            @Html.LabelFor(model => model.DistanciaRecorrida, new { @class = "fw-bold" })<br />
                            @Html.DisplayFor(model => model.DistanciaRecorrida)
                            @Html.HiddenFor(model => model.DistanciaRecorrida)
                        </div>
                    }
                    <div class="clearfix"></div>
                    @if (Model.TipoUbicacion == "Origen")
                    {
                        <h3 class="title-hero">
                            Ubicación Origen Domicilio
                        </h3>
                    }
                    else
                    {
                        <h3 class="title-hero">
                            Ubicación Destino Domicilio
                        </h3>
                    }
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.Calle, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.Calle)
                        @Html.HiddenFor(model => model.Domicilio.Calle)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.CodigoPostal, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.CodigoPostal)
                        @Html.HiddenFor(model => model.Domicilio.CodigoPostal)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.Colonia, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.Colonia)
                        @Html.HiddenFor(model => model.Domicilio.Colonia)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.Estado, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.Estado)
                        @Html.HiddenFor(model => model.Domicilio.Estado)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.Localidad, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.Localidad)
                        @Html.HiddenFor(model => model.Domicilio.Localidad)

                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.Municipio, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.Municipio)
                        @Html.HiddenFor(model => model.Domicilio.Municipio)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.NumeroExterior, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.NumeroExterior)
                        @Html.HiddenFor(model => model.Domicilio.NumeroExterior)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.NumeroInterior, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.NumeroInterior)
                        @Html.HiddenFor(model => model.Domicilio.NumeroInterior)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.Pais, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.Pais)
                        @Html.HiddenFor(model => model.Domicilio.Pais)
                    </div>
                    <div class="form-group col-xs-12 col-sm-3">
                        @Html.LabelFor(model => model.Domicilio.Referencia, new { @class = "fw-bold" })<br />
                        @Html.DisplayFor(model => model.Domicilio.Referencia)
                        @Html.HiddenFor(model => model.Domicilio.Referencia)
                    </div>


                    <div class="clearfix"></div>

                </div>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>

    </div>
    <div class="clearfix"></div>
    <br />
}