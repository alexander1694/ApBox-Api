@model API.Operaciones.ComplementoCartaPorte.PartesTransporte


@using (Html.BeginCollectionItem("TiposFigura.PartesTransportes"))
{
    @Html.ValidationSummary(true, "", new { @class = "label-blue-alt" })

<div class="card shadow-sm">
    <div class="card-header" style="background-color: #6D84C2;">
        <h3 class="card-title text-white">
            Partes Transporte
        </h3>
        <div class="card-toolbar">
            <a href="#" class="icon-separator" onclick="removeDetallePTransporte(this)">
                <!--begin::Svg Icon -->
                <span class="svg-icon svg-icon-3x">
                    <img src="~/_CDN/assets/media/icons/duotune/general/gen034.svg" width="30" height="30" />
                </span>
                <!--end::Svg Icon-->
            </a>
        </div>
    </div>
    <div class="card-body row PTransporte">
        <div class="form-group col-xs-12 col-sm-3">
            @Html.LabelFor(model => model.ParteTransporte, new { @class = "fw-bold" })<br />
            <p id="ParteTransporte">@Model.ParteTransporte</p>
            @Html.HiddenFor(model => model.ParteTransporte)
        </div>
        <div class="clearfix"></div>
    </div>
</div>
<div class="clearfix"></div><br />
}
