            cantidad = cero($("#Conceptos_Cantidad").val());
            multi = cantidad * precio;

            $("#Conceptos_Importe").val(multi);
        }

        function cero(valor) {
            if (valor == "" || valor == "undefined") {
                return valor = 0;
            } else {
                return parseFloat(valor);
            }
        }

