}




<script>
  
    $(document).ready(function() {
        @*var totalesList = @Html.Raw(JsonSerializer.Serialize(Model.MontoCFDI).ToList());*@
        var Mestotal1 = @Model.MontoCFDI;
        var Mestotal2 = @Model.Mes2total;
        var Mestotal3 = @Model.Mes3total;
        var Mestotal4 = @Model.Mes4total;
        var Mestotal5 = @Model.Mes5total;
        var Mestotal6 = @Model.Mes6total;



        var mes1 = '@Model.Mes1';
        var mes2 = '@Model.Mes2';
        var mes3 = '@Model.Mes3';
        var mes4 = '@Model.Mes4';
        var mes5 = '@Model.Mes5';
        var mes6 = '@Model.Mes6';




        var element = document.getElementById('chart_clientes_por_mes');
        var labelColor = KTUtil.getCssVariableValue('--kt-gray-500');
        var borderColor = KTUtil.getCssVariableValue('--kt-gray-200');
        var baseColor = KTUtil.getCssVariableValue('--kt-primary');
        var secondaryColor = KTUtil.getCssVariableValue('--kt-gray-300');

        if (!element) {
            return;
        }

        var options = {
            series: [{

                name: 'Beneficio Neto',
                data: [Mestotal1, Mestotal2, Mestotal3, Mestotal4, Mestotal5, Mestotal6]
            }, {
                    name: 'Ingresos',
                data: [Mestotal1, Mestotal2, Mestotal3, Mestotal4, Mestotal5, Mestotal6]
                }],
            chart: {
                fontFamily: 'inherit',
                type: 'bar',
                height: '350',
                toolbar: {
                    show: false
                }
            },
            plotOptions: {
                bar: {
                    horizontal: false,
                    columnWidth: ['30%'],
                    endingShape: 'rounded'
                },
            },
            legend: {
                show: false
            },
            dataLabels: {
                enabled: false
            },
            stroke: {
                show: true,
                width: 2,
                colors: ['transparent']
            },
            xaxis: {
                categories: [mes1, mes2, mes3, mes4, mes5, mes6],
                axisBorder: {
                    show: false,
                },
                axisTicks: {
                    show: false
                },
                labels: {
                    style: {
                        colors: labelColor,
                        fontSize: '12px'
                    }
                }
            },
            yaxis: {
                labels: {
                    style: {
                        colors: labelColor,
                        fontSize: '12px'
                    }
                }
            },
            fill: {
                opacity: 1
            },
            states: {
                normal: {
                    filter: {
                        type: 'none',
                        value: 0
                    }
                },
                hover: {
                    filter: {
                        type: 'none',
                        value: 0
                    }
                },
                active: {
                    allowMultipleDataPointsSelection: false,
                    filter: {
                        type: 'none',
                        value: 0
                    }
                }
            },
            tooltip: {
                style: {
                    fontSize: '12px'
                },
                y: {
                    formatter: function (val) {
                        return '$' + val + ' Miles'
                    }
                }
            },
            colors: [baseColor, secondaryColor],
            grid: {
                borderColor: borderColor,
                strokeDashArray: 4,
                yaxis: {
                    lines: {
                        show: true
                    }
                }
            }
        };

        var chart = new ApexCharts(element, options);
        chart.render();
    });


</script>



















